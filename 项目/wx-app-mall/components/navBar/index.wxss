.custom_nav {
  width: 100%;
  background: #fff;
}

.custom_nav_box {
  position: fixed;
  width: 100%;
  background: var(--primary-color);
  z-index: 10;
}
.custom_nav_bar{
  position: relative;
}
.custom_nav_box .nav_title {
  font-size: 34rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-weight: 600;
}
.custom_nav_box .custom_nav_icon {
  position: absolute;
  border: .5rpx solid rgba(0, 0, 0, 0.08);
  border-radius: 50%;
  display: flex;
  padding: 0 10rpx;
  align-items: center; 
}
.custom_nav_icon .goback {
  padding: 0 5px;
  font-size: 18px;
  display: flex;
  align-items: center;
}
.custom_nav_icon .home {
  padding: 0 5px 0 12px;
  font-size: 18px;
  display: flex;
  align-items: center;
}