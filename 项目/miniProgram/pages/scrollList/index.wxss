.list-container {
  background: #ffffff;
  padding-top: 44rpx;
  padding-bottom: 0px;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.header {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  width: 100%;
  height: 100rpx;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 44rpx;
  background: #ffffff;
  z-index: 99;
  padding-left: 28rpx;
  padding-right: 28rpx;
}

.price-image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 10rpx;
}

.goods-more {
  position: relative;
  display: flex;
  margin: 32rpx 0rpx;
  width: 100%;
  height: 180rpx;
}

.goods-more-icon {
  height: 180rpx;
  line-height: 180rpx;
  display: flex;
  align-items: center;
  margin-right: 18rpx;
}

.header-content {
  display: flex;
  height: 80rpx;
}

.mulit-selected-content {
  width: 80rpx;
  margin-right: 30px;
}

.search {
  flex: 10.5;
  margin-left: 40rpx;
  background:rgba(245,245,245,1);
  border-radius: 36rpx;
  display: flex;
  align-items: center;
}

.search-icon {
  font-size: 40rpx;
  margin-left: 22rpx;
  margin-right: 16rpx;
  padding-top: 12rpx;
  color: #A3AAB7;
}

.search-text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color:rgba(163,170,183,1);
}

.sequence {
  flex: 1.5;
  display: flex;
  align-items: center;
  margin-left: 10rpx;
}

.squence-img {
  float: right;
  width: 32rpx;
  height: 34rpx;
}

.squence-img image {
  width: 100%;
  height: 100%;
}

.term {
  position: fixed;
  top: 80rpx;
  left: 0;
  z-index: 2;
  width: 100%;
  background: #ffffff;
}

.term-item {
  height: 40rpx;
  display: flex;
  justify-content: space-around;
  margin: 28rpx;
}

.news, .hot, .spot, .select {
  text-align: center;
  font-size: 28rpx;
  color:rgba(163,170,183,1)
}

.active-item {
  font-size: 28rpx;
  color:rgb(82, 71, 71);
}

.active-item-group {
  font-size: 28rpx;
  color: #4F989E;
}

.container-list {
  margin-top: 180rpx;
}

.goodsList-item {
  position: relative;
  display: inline-block;
  width: 332rpx;
  height: 514rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 8rpx 20rpx 0rpx rgba(0,0,0,0.04);
  border-radius:12rpx;
  border: 4rpx solid rgba(245,245,245,1);
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}

.goodsList-item:nth-child(odd) {
  margin-left: 24rpx;
  margin-right: 10rpx;
}

.goodsList-item:nth-child(even) {
  margin-left: 10rpx;
  margin-right: 18rpx;
}

.book-tip {
  font-size: 20rpx;
  color:rgba(236,124,104,1);
}

.spot-tip {
  font-size: 20rpx;
  color:rgba(79,152,158,1);
}

.item-tip {
  position: absolute;
  display: flex;
  justify-content: center;
  top: 12rpx;
  left: 12rpx;
  color: #FF4F989E;
  font-size: 20rpx;
  width: 80rpx;
  height: 36rpx;
  line-height:  36rpx;
  background:rgba(255,255,255,1);
  border-radius: 18rpx;
}

.cart-icon {
  font-size: 40rpx;
}

.icon-hot {
  display: flex;
  width: 120rpx;
  height: 80rpx;
  justify-content: flex-end;
  align-items: center;
}

.item-footer-unit {
  font-size: 20rpx;
}

.item-footer-price {
  height: 40rpx;
  font-size: 24rpx;
  color:rgba(77,77,77,1);
  line-height: 34rpx;
}

.item-name {
  height: 40rpx;
  font-size: 28rpx;
  color:rgba(77,77,77,1);
  line-height:40rpx;
}

.success-tip {
  position: fixed;
  left: 50%;
  top: 50%;
  z-index: 99;
  transform: translateX(-190rpx)  translateY(-90rpx);
  width:380rpx;
  height:180rpx;
  background:rgba(0,0,0,0.7);
  border-radius:12rpx;
  text-align: center;
}

.success-icon {
  width: 48rpx;
  height: 48rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.success-icon image {
  width: 100%;
  height: 100%;
}

.success-text {
  width:288rpx;
  height:34rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:34rpx;
  text-align: center;
  margin: 0 auto;
}

/* 更多款式 */

.mulit-mask {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 5;
  opacity: 1;
  background-color: rgba(0, 0, 0, 0.2);
  height: 0%;
}

.mask {
  display: block;
  transition: heigth 0.5s;
  height: 100%;
}

.mulit-contianer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 0;
  overflow: auto;
  background: #ffffff;
  z-index: 99;
  transition: height 0.5s;
}

.mulit-goods-top {
  display: flex;
  align-items: center;
  height: 120rpx;
  background:rgba(255,255,255,1);
}

.back-icon {
  width: 50rpx;
}

.mulit-title {
  flex: 1;
  display: flex;
  justify-content: center;
  font-weight: bold;
}

.arrow-icon {
  font-size: 40rpx;
  font-weight: bold;
}

.mulit-content {
  position: static;
  max-height: 80vh;
  width: 100%;
  overflow-y: auto;
}

.mulit-contianer-show {
  height: 80vh;
}

.mulit-goods-bottom {
  display: flex;
  justify-content: space-between;
  margin: 0 28rpx;
}

.scroll {
  margin-top: 50rpx;
  margin-bottom: 110rpx;
  height: 84vh;
}


.input-selected {
  width: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.mulit-goods-content {
  padding: 0 28rpx;
}

.goods-more-img {
  width: 180rpx;
  height: 180rpx;
  background:rgba(245,245,245,1);
  border-radius: 12rpx;
  /* border: 2rpx solid rgba(245,245,245,1); */
  overflow: hidden;
  margin-right: 32rpx;
}

.goods-more-content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 400rpx;
  position: relative;
}

.goods-more-contentTop {
  position: relative;
  width: 100%;
  height: 40rpx;
  /* overflow: hidden; */
}

.shopping-books {
  position: absolute;
  top: 12rpx;
  left: 70rpx;
  width:80rpx;
  height:36rpx;
  line-height: 36rpx;
  background:rgba(255,255,255,1);
  border-radius:18rpx;
  text-align: center;
  font-size:20rpx;
  color:rgba(236,124,104,1);
}

.goods-more-contentMiddle {
  flex: 8;
  display: flex;
  justify-content: space-between;
}

.goods-more-contentMiddle-content {
  width: 320rpx;
  height: 80rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.goods-more-contentMiddle-stack {
  width:64rpx;
  height:34rpx;
  font-size:24rpx;
  color:rgba(77,77,77,1);
  line-height:34rpx;
  overflow: hidden;
  text-align: center;
}

.contentMiddle-stack-unit {
  font-size: 20rpx;
}

.goods-more-contentBottom {
  flex: 2;
}

.goods-more-tip {
  position: absolute;
  top: 0;
  right: -60rpx;
  color: #ffffff;
  background:rgba(79,152,158,1);
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  width: 88rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  color:rgba(255,255,255,1);
  text-align: center;
}

.goods-more-title {
  display: inline-block;
  width: 340rpx;
  font-size: 28rpx;
  color:rgba(77,77,77,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods-more-item {
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  margin-top: 2rpx;
  margin-bottom: 2rpx;
  margin-right: 16rpx;
}

.goods-more-price-unit {
  font-size: 20rpx;
  color:rgba(77,77,77,1);
}

.goods-more-price-content {
  font-size: 24rpx;
  color:rgba(77,77,77,1);
}

.goods-more-operating {
  display: flex;
}

.goods-more-reduce {
  width:  40rpx;
  height: 40rpx;
  border: 2rpx solid rgba(77,77,77,1);
  text-align: center;
  line-height: 40rpx;
}

.goods-more-number {
  margin: 0 8rpx;
}

.goods-more-add {
  width: 40rpx;
  height: 40rpx;
  background: rgba(77,77,77,1);
  border: 2rpx solid rgba(77,77,77,1);
  color: #ffffff;
  text-align: center;
  line-height: 40rpx;
}

.goods-more-contentBottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.mulit-goods-footer {
  text-align: center;
  height:100rpx;
  background:rgba(255,255,255,1);
  align-items: center;
}

.price-image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 10rpx;
}

.goods-more-credentials {
  margin-top: 6rpx;
  font-size: 20rpx;
  color:rgba(163,170,183,1);
}

.mulit-goods-cart {
  display: inline-block;
  width: calc(100% - 30px);
  height: 80rpx;
  line-height: 80rpx;
  background:rgba(77,77,77,1);
  border-radius: 40rpx;
  font-size: 28rpx;
  color:rgba(255,255,255,1);
}

.moreText, .mainText {
  display: flex;
  justify-content: center;
}

.mainText {
  margin: 0 auto;
  display: block;
  text-align: center;
  width: 216rpx;
  height: 34rpx;
  margin-top: 80rpx;
  margin-bottom: 80rpx;
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  line-height: 34rpx;
}

.mainScroll {
  height: 85vh;
}

.emptyData {
  margin: 84rpx auto;
  width: 326rpx;
  height: 206rpx;
}

.emptyData image {
  width: 100%;
  height: 100%;
}

.add-shopcart {
  flex: 1;
  display: inline-block;
  background:rgba(245,245,245,1);
  font-size:28rpx;
  font-weight:500;
  color:rgba(77,77,77,1);
}

.buy-now {
  flex: 1;
  display: inline-block;
  background:rgba(77,77,77,1);
  font-size:28rpx;
  font-weight:500;
}

.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 110rpx;
  background: #ffffff;
  width: 100%;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: 0px;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.footer-content {
  display: flex;
  width:690rpx;
  height:80rpx;
  border-radius:80rpx;
  text-align: center;
  line-height: 80rpx;
  color: #ffffff;
  overflow: hidden;
}
.goods-more-tip-1-style{
  background: #8FBA6B;
}
.goods-more-price-unit-1-style{
  color: #C11111;
}
.goods-more-price-1-style{
  color: #C11111;
  font-size: 30rpx;
  font-weight: 400;
}
.goods-more-contentMiddle-1-style{
  height: 50rpx;
}
.goods-stock{
  flex: 1;
  font-size:20rpx;
  color: #999999;
}
.goods-more-operating-1-style{
  position: absolute;
  right: -24rpx;
  bottom: -3rpx;
}
.footer-content-1-style{
  background:linear-gradient(90deg,rgba(221,178,127,1) 0%,rgba(219,176,81,1) 100%);
}
.gb-tag{
  width: 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  background:rgba(212,109,104,1);
  border-radius:2px;
  color: #fff;
  font-size: 20rpx;
  font-weight: 400;
  text-align: center;
  left: 72rpx;
}
.jwelry-number-1-style .van-stepper__input{
  width:90rpx;
  height:48rpx;
  background:rgba(245,245,245,1);
  border-radius:2rpx;
  font-size: 24rpx;
  color: #4D4D4D;
}
.jwelry-number-1-style .van-stepper__minus, .jwelry-number-1-style .van-stepper__plus{
  background-color: inherit;
  color: #000;
}
.jwelry-number-1-style .van-stepper__minus--disabled, .jwelry-number-1-style .van-stepper__plus--disabled {
  color: #c8c9cc;
  color: var(--stepper-button-disabled-icon-color,#c8c9cc);
  font-weight: 400;
}

/* 钻石版 */
.footer-content-diamand {
  background: rgba(255,255,255,1);
}

.add-shopcart-diamand {
  background:rgba(245,245,245,1);
  border-radius:40rpx 0rpx 0rpx 40rpx;
}

/* 黄金版 */
.footer-content-gold {
  background:linear-gradient(90deg,rgba(221,178,127,1) 0%,rgba(219,176,81,1) 100%);
}

.add-shopcart-gold {
  background:rgba(255,250,240,1);
  border:1rpx solid rgba(219,176,86,1);
  color:rgba(219,176,81,1);
  border-radius:40rpx 0rpx 0rpx 40rpx;
}

.buy-now-gold {
  background:linear-gradient(90deg,rgba(221,178,127,1) 0%,rgba(219,176,81,1) 100%);
  color: rgba(255,255,255,1);
}