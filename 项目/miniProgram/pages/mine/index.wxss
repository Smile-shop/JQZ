}
.mine-navigate-cell-1-style{
  font-size:32rpx;
  color: #666;
}
.gold-m-image image{
  /* border: 8rpx solid #f2d079; */
}
.view-icon{
  width: 44rpx;
  height: 44rpx;
  margin-right: 24rpx;
}
.view-icon image{
  width: 100%;
  height: 100%;
}
.img-global-box .title-noLogin-1-style{
  background:linear-gradient(90deg,rgba(221,178,127,1) 0%,rgba(219,176,81,1) 100%);
}
.order-container-gold {
  height:264rpx;
  background:rgba(255,255,255,1);
  /* box-shadow:0px 4rpx 16rpx 0rpx rgba(0,0,0,0.15); */
