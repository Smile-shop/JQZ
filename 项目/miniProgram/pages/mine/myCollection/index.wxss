.collection-wrapper{
  width: 100%;
  height: 100vh;
  background: #fff;
}
.collection-goods{
  margin-bottom: 20rpx;
}
.collection-goods-w{
  position: relative;
}
.collection-box{
  width: 80%;
  height: 400rpx;
  margin: 0 auto;
  padding-top: 80rpx;
  margin-right: 50rpx;

}
.collection-box image{
  width: 100%;
  height: 100% !important;
}
.collection-text{
  margin-top: 40rpx;
  text-align: center;
  font-size: 28rpx;
  color: #4D4D4D;
}
.collection-button{
  margin-top: 70rpx;
  width: 320rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background: #4D4D4D;
  color: #fff;
  font-size: 28rpx;
}

.collection-swipe .delete {
  display: flex;
  width: 130rpx;
  height: 100%;
  font-size: 15px;
  color: #fff;
  text-align: center;
  background-color: #EC7C68;
  justify-content: center;
  align-items: center;
}
.collection-swipe .delete-1-style {
  background-color: #C11111;
}
.collection-content{
  width: 100%;
  height: 216rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  margin: 30rpx 28rpx 0 28rpx;
  position: relative;

}
.collection-left{
  width: 216rpx;
  height: 216rpx;
  position: relative;
}
.collection-huo{
  position: absolute;
  top: 12rpx;
  left: 12rpx;
  padding: 4rpx 20rpx;
  color: #4F989E;
  font-size: 20rpx;
  background: #fff;
  border-radius: 18rpx;
}
.colletcion-huo-color{
  color: #EC7C68;
}
.collection-left image{
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
  border:1px solid #F5F5F5;
  object-fit: cover;
}
.collection-right{
  flex: 1;
  padding-left: 20rpx;
  position: relative;
}
.goods-title{
  font-size: 28rpx;
  color: #4D4D4D;
  width: 448rpx;
  font-weight: 400;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.goods-param{
  color: #A3AAB7;
  font-size: 24rpx;
  min-height: 135rpx;
}
.goods-param::after{
  display:block;
  content: '';
  clear: both;
}
.goods-param text:not(:first-child){
  margin-left: 16rpx;
}
.goods-price{
  width: 448rpx;
  /* position: absolute; */
  bottom: 4rpx;
  color: #4D4D4D;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 52rpx;
  display:flex;
  flex-direction: row;
}
.goods-price text{
  flex: 2;
}
.goods-cart-icon{
  float: right;
  font-size: 40rpx;
  color: #4D4D4D;
}
.offShelves-box{
  position: absolute;
  width: 96%;
  height: 100%;
  background: rgba(255,255,255,0.5);
  z-index: 9;
  top: 0;
  left: 0;
}
.offShelves-view{
  width: 216rpx;
  height: 216rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* margin: 30rpx 28rpx 0 28rpx; */
}
.offShelves-p{
  width: 152rpx;
  height: 152rpx;
  border-radius: 50%;
  background:rgba(0,0,0,0.3);
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
}
.no-more{
  text-align: center;
  padding: 20rpx 0 10rpx;
  font-size: 24rpx;
  color: #A3AAB7;
}
.show_action .van-popup--bottom{
  height: 100vh;
}

.action-top{
  margin: 40rpx 20rpx;
  display: flex;
}
.action-check{
  float:left;
  margin-left: 10rpx;
}
.action-price{
  text-align: right;
  /* margin-right: 40rpx; */
  font-size: 24rpx;
  color: #4D4D4D;
  position: relative;
  flex: 1;
}
.action-price text{
  margin-right: 38rpx;
}
.action-price .action-arrow{
  display: inline-block;
  width: 18rpx;
  height: 30rpx;
  position: absolute;
  right:0;
  top: -14rpx;
}
.action-arrow .arrow-up{
  display: inline-block;
  width: 0;
  height: 0;
	border-right: 8rpx solid transparent;
	border-left: 8rpx solid transparent;
}
.action-arrow .arrow-down{
  display: inline-block;
  width: 0;
  height: 0;
	border-right: 8rpx solid transparent;
	border-left: 8rpx solid transparent;
	/* border-top: 18rpx solid #ddd; */
  margin-bottom: 10rpx;
}
.action-arrow .up-sected-color{
  border-bottom: 16rpx solid #4D4D4D;
}
.action-arrow .up-unsected-color{
  border-bottom: 16rpx solid #ddd;
}
.action-arrow .down-sected-color{
  border-top: 16rpx solid #4D4D4D;
}
.action-arrow .down-unsected-color{
  border-top: 16rpx solid #ddd;
}
.shopping-item {
  display: flex;
  height: 180rpx;
  padding: 0 28rpx 28rpx 28rpx;
}
.shopping-item:not(:last-child) {
  margin-bottom: 32rpx;
}
.delete-button {
  background-color: #EC7C68;
  height: 180rpx;
  line-height: 180rpx;
  text-align: center;
  width: 60px;
  font-size: 28rpx;
  color: #fff;
}
.shopping-item-img {
  width: 180rpx;
  height: 180rpx;
  margin: 0 20rpx;
  flex-shrink: 0;
  background:rgba(245,245,245,1);
  border-radius:6px;
  border:1px solid rgba(245,245,245,1);
}
.shopping-item-info {
  flex-grow: 1;
  /* background-color: red; */
  position: relative;
}
.shopping-bug{
  position: absolute;
  top: 4rpx;
  right:-14rpx;
  font-size: 20rpx;
  background: #4F989E;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  color: #fff;
  padding: 6rpx 10rpx 6rpx 16rpx;
}

.shopping-item-name {
  width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  font-weight: 400;
  color: #4D4D4D;
}
.shopping-item-code, .shopping-item-detail {
  font-size: 24rpx;
  color:  #A3AAB7;
  line-height: 50rpx;
  height: 90rpx;
  overflow: hidden;
}
.shopping-item-detail text:not(:first-child){
  margin-left: 16rpx;
}
.shopping-item-number {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.add-shopCart{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  z-index: 999;
  background: #fff;
}
.add-shopCart button{
  width:690rpx;
  height:80rpx;
  background:rgba(77,77,77,1);
  border-radius:40rpx;
  font-size: 28rpx;
  color: #fff;
  margin-top: 9rpx;
}
.goods-price-1-style{
  color: #C11111;
  width: 430rpx;
}
.goods-price-1-style .goods-price-num{
  font-size: 30rpx;
}
.price-null-1-style .goods-price-num{
  font-size: 30rpx;
}
.cart-o-1-style{
  color: #C11111;
  position: relative;
  top: 20rpx;
}
.collection-wrapper-1-style{
  background: #F5F5F5;
  height: auto;
}
.collection-goods-1-style{
  margin: 20rpx 19rpx 20rpx 20rpx;
  background: #fff;
  border-radius:20rpx;
  padding-bottom: 20rpx;
  /* margin-top: -150rpx; */
  position: relative;
  padding-right: 4rpx;
}
.collection-content-1-style{
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.goods-stock{
  font-size:20rpx;
  color: #999;
  font-weight: 400;
}
.goods-param-1-style{
  min-height: 100rpx;
  position: relative;
}
.collection-huo-1-style{
  top: inherit;
  bottom: 0;
  left: -16rpx;
  background:rgba(212,109,104,1);
  border-radius:2rpx;
  font-size:20rpx;
  font-weight:400;
  color: #fff;
}
.collection-bground{
  width: 100%;
  height: 154rpx;
  position:fixed;
  top:-2rpx;
  left: 0;
}
.collection-gold-img{
  width: 100%;
  height: 100%;
  position: relative
}
.collection-gold-box{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 120rpx;
}
.collect-box-image{
  width:305rpx;
  height:206rpx;
}
.collect-box-image image{
  width: 100%;
  height: 100%;
}
.collection-text-1-style{
  color: #666;
}