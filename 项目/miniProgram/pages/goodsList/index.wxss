/* pages/goodsList/index.wxss */
.header {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2;
  width: 100%;
  background: #ffffff;
}

.header-content {
  display: flex;
  height: 72rpx;
  margin-bottom: 8rpx;
  margin-top: 20rpx;
}

.search {
  flex: 10.5;
  margin-left: 40rpx;
  background:rgba(245,245,245,1);
  border-radius: 36rpx;
  display: flex;
  align-items: center;
}

.search-icon {
  width: 32rpx;
  height: 32rpx;
  margin: 0 30rpx;
  color:rgba(163,170,183,1);
}

.search-text {
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 530rpx;
}

.sequence {
  flex: 1.5;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}

.squence-img {
  float: right;
  width: 32rpx;
  height: 34rpx;
}

.squence-img image {
  width: 100%;
  height: 100%;
}

.term {
  position: fixed;
  top: 99rpx;
  left: 0;
  z-index: 2;
  width: 100%;
  background: #ffffff;
}

.term-item {
  height: 40rpx;
  display: flex;
  justify-content: space-around;
  margin: 28rpx;
}

.news, .hot, .spot, .select {
  text-align: center;
  font-size: 28rpx;
  color:rgba(163,170,183,1);
  width: 120rpx;
  height: 56rpx;
  line-height: 56rpx;
}

.select {
  display: flex;
  align-items: center;
}

.spot {
  width: 166rpx;
}

.active-item {
  font-size: 28rpx;
  color:rgb(82, 71, 71);
}

.active-item-group {
  font-size: 28rpx;
  color: #4F989E;
}

.container-list {
  margin-top: 190rpx;
  background-color: #ffffff;
}

.goodsList-item {
  position: relative;
  display: inline-block;
  width: 332rpx;
  height: 514rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 8rpx 20rpx 0rpx rgba(0,0,0,0.04);
  border-radius:12rpx;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  overflow: hidden;
}

.goodsList-item:nth-child(odd) {
  margin-left: 30rpx;
  margin-right: 14rpx;
}

.goodsList-item:nth-child(even) {
  margin-left: 14rpx;
  margin-right: 18rpx;
}

.book-tip {
  font-size: 20rpx;
  color:rgba(236,124,104,1);
}

.spot-tip {
  font-size: 20rpx;
  color:rgba(79,152,158,1);
}

.group-color {
  width:166rpx;
  height:56rpx;
  background:rgba(245,245,245,1);
  border-radius:38rpx;
}

.item-tip {
  position: absolute;
  display: flex;
  justify-content: center;
  top: 12rpx;
  left: 12rpx;
  color: #FF4F989E;
  font-size: 20rpx;
  width: 80rpx;
  height: 36rpx;
  line-height:  36rpx;
  background:rgba(255,255,255,1);
  border-radius: 18rpx;
}

.cart-icon {
  font-size: 40rpx;
}

.icon-hot {
  display: flex;
  width: 120rpx;
  /* height: 80rpx; */
  justify-content: flex-end;
  align-items: center;
  position: relative;
}

.price-limit {
  max-width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-footer-unit {
  font-size: 24rpx;
  font-weight: 400;
}

.item-footer-price {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 600;
  color:rgba(77,77,77,1);
  line-height: 34rpx;
}

.item-name {
  height: 40rpx;
  line-height:40rpx;
  width: 312rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-name-text {
  font-size: 28rpx;
  color:rgba(77,77,77,1);
}

.coloumText-item-name {
  height: 40rpx;
  font-size: 28rpx;
  color:rgba(77,77,77,1);
  line-height:40rpx;
  width: 420rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.image-wrapper {
  width: 332rpx;
  height: 332rpx;
  background:rgba(245,245,245,1);
  border-radius: 12rpx 12rpx 0px 0px;
}

.rowImage {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 10rpx;
}

.rowImage .item-name {
  height: 40rpx;
  line-height: 40rpx;
  flex: 1;
  font-size: 28rpx;
  color:rgba(77,77,77,1);
  margin-bottom: 4rpx;
}

.rowImage .item-distinct {
  flex: 10;
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  display: flex;
  justify-content: space-between;
}

.row-distinct-text {
  flex: 9;
  height: 40rpx;
  width: 136rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.row-distinct-number {
  text-align: right;
  width:64rpx;
  height:34rpx;
  font-size:24rpx;
  color:rgba(77,77,77,1);
  line-height:34rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.row-distinct-number-unit {
  font-size: 20rpx;
}

.row-distinct-textItem {
  margin: 5rpx 6rpx;
}

.row-price-container {
  display: flex;
  height: 40rpx;
  line-height: 40rpx;
}

.row-price-item {
  width: 170rpx;
  font-size: 28rpx;
  font-weight: 600;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.row-price-unit {
  font-size: 24rpx;
  font-weight: 400;
}

.rowImage .item-footer {
  flex: 1;
  display: flex;
  justify-content: space-between;
  color:rgba(77,77,77,1);
  margin-top: 30rpx;
}
.item-distinct {
  font-size: 24rpx;
}

.item-footer {
  display: flex;
}

.columList-item {
  position: relative;
  display: flex;
  height: 216rpx;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  margin-left: 28rpx;
  margin-right: 28rpx;
  background:rgba(255,255,255,1);
  box-shadow: 0px 8rpx 20rpx 0px rgba(0,0,0,0.04);
  border-radius: 12rpx;
  /* border: 2rpx solid rgba(245,245,245,1); */
  overflow: hidden;
}

.columList-item:last-child {
  margin-bottom: 0;
}

.columImage {
  width: 216rpx;
  height: 216rpx;
  overflow: hidden;
  flex-shrink: 0;
  background:rgba(245,245,245,1);
}

.columImage image {
  width: 100%;
  height: 100%;
}

.columText {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 100%;
  padding: 20rpx 40rpx;
}

.columText .item-name {
  flex: 1;
  font-size: 28rpx;
  color:rgba(77,77,77,1);
}

.columText .item-distinct {
  flex: 1;
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  display: flex;
}

.item-distinct-text {
  flex: 9;
  width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-distinct-textItem {
  margin: 0 6rpx;
}

.item-distinct-number {
  font-size: 22rpx;
  text-align: right;
  width: 64rpx;
  height: 34rpx;
  color: rgba(77,77,77,1);
  line-height: 34rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-distinct-num-site {
  font-size: 24rpx;
  text-align: right;
}

.item-distinct-num-unit {
  font-size: 20rpx;
}

.columText .item-footer {
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/* 更多款式 */

.mulit-mask {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 5;
  opacity: 1;
  background-color: rgba(0, 0, 0, 0.2);
  height: 0%;
}

.mask {
  display: block;
  transition: heigth 0.5s;
  height: 100%;
}

.mulit-contianer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 0;
  overflow: auto;
  background: #ffffff;
  z-index: 99;
  transition: height 0.5s;
}

.mulit-goods-top {
  display: flex;
  align-items: center;
  height: 120rpx;
  background:rgba(255,255,255,1);
}

.back-icon {
  width: 50rpx;
}

.mulit-title {
  flex: 1;
  display: flex;
  justify-content: center;
  font-weight: bold;
}

.arrow-icon {
  font-size: 40rpx;
  font-weight: bold;
}

.mulit-content {
  position: static;
  max-height: 80vh;
  width: 100%;
  overflow-y: auto;
}

.mulit-contianer-show {
  height: 80vh;
}

.mulit-goods-content {
  margin-top: 20rpx;
  overflow-y: scroll;
}

.mulit-goods-bottom {
  display: flex;
  justify-content: space-between;
  margin: 0 28rpx;
}

.scroll {
  height: 60vh;
}

.goods-more {
  display: flex;
  margin: 32rpx 28rpx;
}

.input-selected {
  width: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.goods-more-icon {
  flex: 1;
  height: 180rpx;
  line-height: 180rpx;
  display: flex;
  align-items: center;
}

.goods-more-images {
  flex: 3;
}

.goods-more-img {
  width: 180rpx;
  height: 180rpx;
  background:rgba(245,245,245,1);
  border-radius: 12rpx;
  border: 2rpx solid rgba(245,245,245,1);
}

.goods-more-img image {
  width: 100%;
  height: 100%;
}

.goods-more-content {
  flex: 8;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 30rpx;
}

.goods-more-contentTop {
  position: relative;
  flex: 2;
}

.goods-more-contentMiddle {
  flex: 8;
}

.goods-more-contentBottom {
  flex: 2;
}

.goods-more-tip {
  position: absolute;
  top: 0;
  right: -30rpx;
  color: #ffffff;
  background:rgba(79,152,158,1);
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  width: 86rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  color:rgba(255,255,255,1);
  text-align: center;
}

.goods-more-title {
  font-size: 28rpx;
  color:rgba(77,77,77,1);
}

.goods-more-item {
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  margin-top: 2rpx;
  margin-bottom: 2rpx;
  margin-right: 16rpx;
}

.goods-more-price-unit {
  font-size: 20rpx;
  color:rgba(77,77,77,1);
}

.goods-more-price-content {
  font-size: 24rpx;
  color:rgba(77,77,77,1);
}

.goods-more-operating {
  display: flex;
}

.goods-more-reduce {
  width:  40rpx;
  height: 40rpx;
  border: 2rpx solid rgba(77,77,77,1);
  text-align: center;
  line-height: 40rpx;  
}

.goods-more-number {
  margin: 0 8rpx;
}

.goods-more-add {
  width: 40rpx;
  height: 40rpx;
  background: rgba(77,77,77,1);
  border: 2rpx solid rgba(77,77,77,1);
  color: #ffffff;
  text-align: center;
  line-height: 40rpx;  
}

.goods-more-contentBottom {
  display: flex;
  justify-content: space-between;
}

.mulit-goods-footer {
  text-align: center;
  height:100rpx;
  background:rgba(255,255,255,1);
  align-items: center;
}

.price-image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 10rpx;
}

.goods-more-credentials {
  margin-top: 6rpx;
  font-size: 20rpx;
  color:rgba(163,170,183,1);
}

.mulit-goods-cart {
  display: inline-block;
  width: calc(100% - 30px);
  height: 80rpx;
  line-height: 80rpx;
  background:rgba(77,77,77,1);
  border-radius: 40rpx;
  font-size: 28rpx;
  color:rgba(255,255,255,1);
}

.moreText, .mainText {
  display: flex;
  justify-content: center;
}

.mainText {
  margin: 0 auto;
  display: block;
  text-align: center;
  width: 216rpx;
  height: 34rpx;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  font-size: 24rpx;
  color:rgba(163,170,183,1);
  line-height: 34rpx;
}

.mainScroll {
  height: 85vh;
}

.emptyData {
  margin: 84rpx auto;
  width: 326rpx;
  height: 206rpx;
}

.emptyData image {
  width: 100%;
  height: 100%;
}

.filter-icon {
  width: 32rpx;
  height: 34rpx;
  margin-left: 10rpx;
}
