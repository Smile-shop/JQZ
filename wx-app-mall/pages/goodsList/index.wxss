.mulit-mask {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 5;
  opacity: 1;
  background-color: rgba(0, 0, 0, 0.2);
  height: 0%;
}

.mask {
  display: block;
  transition: heigth 0.5s;
  height: 100%;
}

.mulit-contianer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 0;
  overflow: auto;
  background: #ffffff;
  z-index: 99;
  transition: height 0.5s;
}

