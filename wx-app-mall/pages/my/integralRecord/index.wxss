/* pages/shop/index.wxss */
.mine-wrapper{
  height: 100vh;
  background: #ffffff;
  position: relative;
}
.content-view{
  background: #FF4646;
  color: #ffffff;
}
.mine-img{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 200rpx;
  align-items: center;
  justify-content: flex-end;
  padding-bottom: 60rpx;
}

.mine-img .title{
  background-color: #FF4646;
  display: flex;
  height:44rpx;
  font-size:60rpx;
  font-weight:600;
  color: #ffffff;
  line-height:44rpx;
  align-items: center;
  justify-content: center;
}

.mine-img .m-image{
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.m-image image{
  width:40rpx;
  height:40rpx;
  margin-right: 10rpx;
}

.content-detail {
  width: 100%;
  background-color: #ffffff;
}

.middle {
  width: 100%;
  height: 30rpx;
  background-color: #FF4646;
  /* margin-bottom: -2rpx; */
  position: absolute;
  z-index: 9999;
}

.middle-inner {
  width: 100%;
  height: 30rpx;
  border-radius: 30rpx 30rpx 0 0 ;
  overflow: hidden;
  /* background-color: #ffffff; */
  background-color: #ffffff;
}

.content-item {
  display: flex;
  justify-content: center;
  border-top: 1rpx solid #eeeeee;
}

.empty {
  margin-top: 150rpx;
  width: 320rpx;
  height: 234rpx;
}


.empty image {
  width: 100%;
  height: 100%;
}