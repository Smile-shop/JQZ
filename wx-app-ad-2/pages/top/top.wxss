page {
  height: 100vh;
  overflow: hidden;
}

.loading {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 30vw 10vw;
}

.loading > .iconfont {
  font-size: 8vw;
  color: #FC4C39;
  animation-name: rotate;
  animation-duration: 1s;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
  transform-origin: center center;
  margin-bottom: 4vw;
}

.loading > .tip {
  color: #AAAAAA;
  font-size: 3.2vw;
}

.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 18vw 10vw;
}

.empty > image {
  width: 50vw;
  height: 35vw;
  margin-bottom: 8vw;
}

.empty > .tip {
  color: #AAAAAA;
  font-size: 3.7vw;
}

@keyframes rotate {
  100% {
    transform: rotate(360deg);
  }
}

.container > .header > image {
  height: 50vw;
  width: 100%;
  object-fit: cover;
  object-position: center;
}

.main {
  transform: translateY(-5vw);
  overflow: hidden;
  margin-bottom: 17vw;
  background: #FFFFFF;
  border-radius: 4vw 4vw 0vw 0vw;
}

.tab {
  display: flex;
  border-bottom: 1px solid #EEEEEE;
}

.tab > .tab-item {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  position: relative;
  flex-basis: 50%;
  height: 13vw;
  font-size: 3.7vw;
  color: #4D4D4D;
  background-color: #fff;
}

.tab > .tab-item.active {
  font-weight: bold;
  color: #FC4C39;
}

.tab > .tab-item.active::before {
  position: absolute;
  bottom: 0;
  left: calc(50% - 3.2vw);
  border-radius: 1vw;
  height: 1vw;
  width: 6.4vw;
  background-color: #FC4C39;
  display: block;
  content: "";
}

.tab-view {
  height: calc(100vh - 17vw - 55vw - 13vw + 5vw);
}

.tab-view .tab-view-content {
  padding: 3vw 8vw 8vw 8vw;
}

.popularity-top .winning {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 5vw 0 0 0;
  margin-bottom: 9vw;
}

.popularity-top .winning > .winning-item {
  display: flex;
  flex-shrink: 0;
  width: 25vw;
  flex-direction: column;
  align-items: center;
}

.popularity-top .winning > .winning-item > .avatar {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 2.4vw;
  border-radius: 50%;
  height: 15vw;
  width: 15vw;
}

.popularity-top .winning > .winning-item.top-1 > .avatar {
  display: flex;
  align-items: center;
  background-color: #FFB94E;
  height: 17vw;
  width: 17vw;
}

.popularity-top .winning > .winning-item.top-1 > .avatar > .avatar-image {
  height: 15vw;
  width: 15vw;
}

.popularity-top .winning > .winning-item.top-1 > .avatar {
  background-color: #FFB94E;
}

.popularity-top .winning > .winning-item.top-0 > .avatar {
  background-color: #7DB5F6;
}

.popularity-top .winning > .winning-item.top-2 > .avatar {
  background-color: #FC4C39;
}

.popularity-top .winning > .winning-item > .avatar > .crown {
  position: absolute;
  top: -4vw;
  width: 7.6vw;
  height: 4.6vw;
}

.popularity-top .winning > .winning-item > .avatar > text {
  position: absolute;
  color: #FFF;
  top: -7vw;
  font-size: 3vw;
}

.popularity-top .winning > .winning-item > .avatar > .avatar-image {
  border-radius: 50%;
  width: 13vw;
  height: 13vw;
  object-fit: cover;
  object-position: center center;
  background-color: #fff;
}

.popularity-top .winning > .winning-item > .name {
  text-align: center;
  width: 25vw;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 2.4vw;
  font-size: 3.2vw;
  font-weight: bold;
  color: #333333;
}

.popularity-top .winning > .winning-item > .value {
  font-size: 3.2vw;
  font-weight: bold;
}

.popularity-top .winning > .winning-item.top-1 > .value {
  font-size: 3.2vw;
  color: #FFB94E;
}

.popularity-top .winning > .winning-item.top-0 > .value {
  font-size: 3.2vw;
  color: #7DB5F6;
}

.popularity-top .winning > .winning-item.top-2 > .value {
  font-size: 3.2vw;
  color: #FC4C39;
}

.top-table > .top-table-header {
  margin-bottom: 3vw;
  display: flex;
  font-size: 3.2vw;
  color: #999999;
}

.top-table > .top-table-header > .ranking {
  width: 10vw;
  text-align: center;
}

.top-table > .top-table-header > .nickname {
  flex-grow: 1;
  text-align: center;
}

.top-table > .top-table-header > .popularity {
  width: 13vw;
  text-align: center;
}

.top-table > .top-table-body > .top-table-row {
  display: flex;
  align-items: center;
  padding: 1.6vw 0;
}

.top-table > .top-table-body > .top-table-row > .ranking {
  width: 10vw;
  text-align: center;
  font-size: 3.2vw;
  color: #999999;
}

.top-table > .top-table-body > .top-table-row > .nickname {
  display: flex;
  align-items: center;
  margin-left: 20vw;
  flex-grow: 1;
  font-size: 3.5vw;
  color: #333333;
}

.top-table > .top-table-body > .top-table-row > .nickname > image {
  margin-right: 3.2vw;
  border-radius: 50%;
  object-position: center center;
  object-fit: cover;
  height: 8vw;
  width: 8vw;
}

.top-table > .top-table-body > .top-table-row > .value {
  width: 13vw;
  text-align: center;
  color: #333333;
  font-size: 3.7vw;
  font-weight: bold;
}

.help-table > .help-table-header {
  display: flex;
  justify-content: space-between;
  font-size: 3.2vw;
  color: #999999;
}

.help-table > .help-table-body > .help-table-row {
  display: flex;
  align-items: center;
  padding: 3.5vw 0;
}

.help-table > .help-table-body > .help-table-row > .ranking {
  flex-basis: 30%;
  font-size: 3.2vw;
  color: #999999;
}

.help-table > .help-table-body > .help-table-row > .nickname {
  display: flex;
  align-items: center;
  flex-basis: 40%;
  font-size: 3.5vw;
  color: #333333;
}

.help-table > .help-table-body > .help-table-row > .nickname > image {
  margin-right: 3.2vw;
  border-radius: 50%;
  object-position: center center;
  object-fit: cover;
  height: 8vw;
  width: 8vw;
}

.help-table > .help-table-body > .help-table-row > .value {
  white-space: nowrap;
  flex-basis: 30%;
  text-align: end;
  color: #999999;
  font-size: 2.5vw;
}

.container > .footer {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  align-items: center;
  padding: 0 3.7vw;
  height: 17vw;
  background: #FFFFFF;
  box-shadow: 0px 0px 1vw 0px rgba(0,0,0,0.1);
  background-color: #fff;
}

.container > .footer > .left {
  display: flex;
  align-items: center;
  margin-right: 4vw;
}

.container > .footer > .left > image {
  object-fit: cover;
  object-position: center center;
  border-radius: 50%;
  height: 11.5vw;
  width: 11.5vw;
}

.container > .footer > .middle {
  flex-grow: 1;
  margin-right: 3vw;
}

.container > .footer > .middle > .name {
  margin-bottom: 1vw;
  font-size: 4.27vw;
  font-weight: bold;
  color: #333333;
}

.container > .footer > .middle .info-list {
  font-size: 3.7vw;
  color: #999999;
}

.container > .footer > .middle .info-list-item:first-of-type {
  margin-right: 4vw;
}

.container > .footer > .middle .info-list-item .value {
  font-weight: bold;
  color: #FC4C39;
}

.container > .footer > .right > button {
  width: auto;
  border-radius: 10vw;
  padding: 2vw 6vw;
  font-size: 3.7vw;
  color: #FFE898;
  font-weight: normal;
  background: linear-gradient(180deg, #FD6452 0%, #DC2E1C 100%);
}