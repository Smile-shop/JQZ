/* components/order/empty/index.wxss */
.empty-container {
  display: flex;
  align-items: center;
  flex-direction: column;
  background-color: rgba(255, 255, 255, 1);
  height: calc(100vh - 92rpx);
}

.split {
  display: block;
  height:20rpx;
  width: 100%;
  background:rgba(245,245,245,1);
  margin-bottom: 160rpx;
}

.empty-img {
  display: flex;
  justify-content: center;
  width: 296rpx;
  height: 230rpx;
}

.empty-text {
  width:280rpx;
  height:40rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(163,170,183,1);
  line-height:40rpx;
  margin-top: 80rpx;
}